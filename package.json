{
  "name": "convex-panel",
  "version": "0.2.14",
  "description": "A React component for monitoring and debugging Convex applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./utils/getConvexToken": {
      "types": "./dist/utils/getConvexToken.d.ts",
      "import": "./dist/utils/getConvexToken.js",
      "require": "./dist/utils/getConvexToken.js",
      "default": "./dist/utils/getConvexToken.js"
    }
  },
  "files": [
    "dist",
    "examples",
    "README.md"
  ],
  "scripts": {
    "copy-src": "node copy-src.js",
    "build": "npm run copy-src && rollup -c",
    "dev": "npm run copy-src && rollup -c -w",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "convex",
    "react",
    "debug",
    "panel",
    "monitoring"
  ],
  "author": "Robert Alvarez",
  "license": "MIT",
  "peerDependencies": {
    "convex": ">=1.25.4",
    "next": ">=15.1.0",
    "react": ">=19.1.0",
    "react-dom": ">=19.1.0"
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.87",
    "@emotion/is-prop-valid": "^1.3.1",
    "@monaco-editor/react": "^4.7.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/recharts": "^1.8.29",
    "@vitejs/plugin-react": "^4.7.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "debounce": "^1.2.1",
    "framer-motion": "^12.23.6",
    "lodash": "^4.17.21",
    "lucide-react": "^0.479.0",
    "monaco-editor": "^0.45.0",
    "react-hotkeys-hook": "^4.6.2",
    "react-use": "^17.6.0",
    "react-window": "^1.8.11",
    "recharts": "^2.15.4",
    "swr": "^2.3.4",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@types/debounce": "^1.2.4",
    "@types/node": "^22.16.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-window": "^1.8.8",
    "convex": "^1.25.4",
    "copyfiles": "^2.4.1",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "next": "^15.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^5.0.10",
    "rollup": "^3.29.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-url": "^3.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robertalv/convex-panel.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/robertalv/convex-panel/issues"
  },
  "homepage": "https://convexpanel.dev"
}
